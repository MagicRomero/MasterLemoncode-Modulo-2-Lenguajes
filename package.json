{
  "name": "lemoncode-tools",
  "version": "0.1.0",
  "description": "A set of tools that made working with javascript objects easily",
  "main": "index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "^.+\\.spec\\.ts$"
  },
  "keywords": [
    "lemoncode",
    "typescript",
    "tools",
    "array",
    "object",
    "functional",
    "inmutable"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "jest": "^26.6.1",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5"
  }
}
